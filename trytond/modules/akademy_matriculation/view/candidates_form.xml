<form>
    <group id="candidate" colspan="4" col="4">   
        <label name="party"/>
        <field name="party"/>  
        <label name="code"/>
        <field name="code"/> 
        <label name="institution"/>
        <field name="institution"/>
        <label name="academic_level"/>
        <field name="academic_level"/>
        <label name="area"/>
        <field name="area"/>
        <label name="course"/>
        <field name="course"/>
    </group>            
    <group id="education" colspan="1" col="6">  
        <label name="date_start"/>
        <field name="date_start"/>
        <label name="date_end"/>
        <field name="date_end"/> 
        <label name="average"/>
        <field name="average"/>
    </group>
    <notebook colspan="4">
        <page string="Candidaturas" id="applications_candidate">
            <field name="applications" mode="tree,form" colspan="4"
                view_ids="akademy_matriculation.candidate_applications_view_tree,akademy_matriculation.candidate_applications_view_form"/>
        </page>
    </notebook>
</form>