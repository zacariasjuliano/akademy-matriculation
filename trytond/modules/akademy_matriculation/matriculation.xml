<tryton>
    <data>
        <menuitem name="Matrículas" id="akademy_registrations" sequence="2"
            parent="akademy_classe.akademy_ga"/>
        
        <!-- start candidates -->
        <record model="ir.ui.view" id="candidates_view_form">
            <field name="model">akademy_matriculation.candidates</field>
            <field name="type">form</field>
            <field name="name">candidates_form</field>
        </record>
        <record model="ir.ui.view" id="candidates_view_list">
            <field name="model">akademy_matriculation.candidates</field>
            <field name="type">tree</field>
            <field name="name">candidates_list</field>
        </record>
        <record model="ir.action.act_window" id="act_candidates">
            <field name="name">Candidatos</field>
            <field name="res_model">akademy_matriculation.candidates</field>
        </record>
        <record model="ir.action.act_window.view" id="act_candidates_view_form">
            <field name="sequence" eval="12"/>
            <field name="view" ref="candidates_view_form"/>
            <field name="act_window" ref="act_candidates"/>
        </record>
        <record model="ir.action.act_window.view" id="act_candidates_view_list">
            <field name="sequence" eval="11"/>
            <field name="view" ref="candidates_view_list"/>
            <field name="act_window" ref="act_candidates"/>
        </record>
        <menuitem name="Candidatos" parent="akademy_registrations" id="akademy_candidates" 
            sequence="10" action="act_candidates" icon="tryton-party"/>        
        
        <!-- start candidate_applications -->
        <record model="ir.ui.view" id="candidate_applications_view_form">
            <field name="model">akademy_matriculation.applications</field>
            <field name="type">form</field>
            <field name="name">applications_form</field>
        </record>
        <record model="ir.ui.view" id="candidate_applications_view_tree">
            <field name="model">akademy_matriculation.applications</field>
            <field name="type">tree</field>
            <field name="name">applications_list</field>
        </record>
        <record model="ir.action.act_window" id="act_candidate_applications">
            <field name="name">Candidaturas</field>
            <field name="res_model">akademy_matriculation.applications</field>
        </record>
        <record model="ir.action.act_window.view" id="act_candidate_applications_view_tree">
            <field name="sequence" eval="15"/>
            <field name="view" ref="candidate_applications_view_tree"/>
            <field name="act_window" ref="act_candidate_applications"/>
        </record>
        <menuitem name="Candidaturas" parent="akademy_registrations" id="akademy_candidate_applications" 
            sequence="14" action="act_candidate_applications"/>                
                
        <!-- start applications_result -->
        <record model="ir.ui.view" id="applications_result_view_form">
            <field name="model">akademy_matriculation.applications.result</field>
            <field name="type">form</field>
            <field name="name">candidate_applications_result_form</field>
        </record>
        <record model="ir.ui.view" id="applications_result_view_tree">
            <field name="model">akademy_matriculation.applications.result</field>
            <field name="type">tree</field>
            <field name="name">candidate_applications_result_list</field>
        </record>
        <record model="ir.action.act_window" id="act_applications_result">
            <field name="name">Resultados</field>
            <field name="res_model">akademy_matriculation.applications.result</field>
        </record>
        <record model="ir.action.act_window.view" id="act_applications_result_view_tree">
            <field name="sequence" eval="19"/>
            <field name="view" ref="applications_result_view_tree"/>
            <field name="act_window" ref="act_applications_result"/>
        </record>
        <menuitem name="Resultados" parent="akademy_registrations" id="akademy_applications_result"
            sequence="18" action="act_applications_result"/>                    
        
        <!-- start student_transfer -->
        <record model="ir.ui.view" id="student_transfer_view_form">
            <field name="model">akademy_matriculation.student.transfer</field>
            <field name="type">form</field>
            <field name="name">student_transfer_form</field>
        </record>
        <record model="ir.ui.view" id="student_transfer_view_list">
            <field name="model">akademy_matriculation.student.transfer</field>
            <field name="type">tree</field>
            <field name="name">student_transfer_list</field>
        </record>
        <record model="ir.action.act_window" id="act_student_transfer">
            <field name="name">Transferências</field>
            <field name="res_model">akademy_matriculation.student.transfer</field>
        </record>
        <record model="ir.action.act_window.view" id="act_student_transfer_view_form">
            <field name="sequence" eval="24"/>
            <field name="view" ref="student_transfer_view_form"/>
            <field name="act_window" ref="act_student_transfer"/>
        </record>
        <record model="ir.action.act_window.view" id="act_student_transfer_view_list">
            <field name="sequence" eval="23"/>
            <field name="view" ref="student_transfer_view_list"/>
            <field name="act_window" ref="act_student_transfer"/>
        </record>
        <menuitem name="Transferências" parent="akademy_registrations" id="akademy_student_transfer" 
            sequence="22" action="act_student_transfer"/>            
        
        <!-- start student_transfer_discipline -->
        <record model="ir.ui.view" id="student_transfer_discipline_view_form">
            <field name="model">akademy_matriculation.student.transfer.discipline</field>
            <field name="type">form</field>
            <field name="name">student_transfer_discipline_form</field>
        </record>
        <record model="ir.ui.view" id="student_transfer_discipline_view_list">
            <field name="model">akademy_matriculation.student.transfer.discipline</field>
            <field name="type">tree</field>
            <field name="name">student_transfer_discipline_tree</field>
        </record> 
        
        <!-- start matriculation -->
        <record model="ir.action.wizard" id="act_matriculation_wizard">
            <field name="name">Matrícular</field>
            <field name="wiz_name">akademy_matriculation.wizmatriculation.create</field>
        </record>
        <record model="ir.ui.view" id="act_matriculation_wizard_from">
            <field name="model">akademy_matriculation.wizmatriculation.create.start</field>
            <field name="type">form</field>
            <field name="name">matriculation_wizcreate_form</field>            
        </record>
        <menuitem action="act_matriculation_wizard" parent="akademy_registrations" id="akademy_matriculation_student" 
            sequence="28"/>  

        <!-- start association_discipline -->
        <record model="ir.action.wizard" id="act_associationdiscipline_wizard">
            <field name="name">Associar disciplina</field>
            <field name="wiz_name">akademy_matriculation.wizassociatiodiscipline.create</field>
        </record>
        <record model="ir.ui.view" id="act_associationdiscipline_wizard_from">
            <field name="model">akademy_matriculation.wizassociatiodiscipline.create.start</field>
            <field name="type">form</field>
            <field name="name">association_discipline_wizcreate_form</field>            
        </record>
        <menuitem action="act_associationdiscipline_wizard" parent="akademy_registrations" id="akademy_associationdiscipline_wiz" 
            sequence="30"/>    

        <!-- start application_avaliation -->
        <record model="ir.action.wizard" id="act_application_avaliation_wizard">
            <field name="name">Avaliar candidatura</field>
            <field name="wiz_name">akademy_matriculation.wizapplication_avaliation.create</field>
        </record>
        <record model="ir.ui.view" id="act_application_avaliation_wizard_from">
            <field name="model">akademy_matriculation.wizapplication_avaliation.create.start</field>
            <field name="type">form</field>
            <field name="name">application_avaliation_wizcreate_form</field>            
        </record>
        <menuitem action="act_application_avaliation_wizard" parent="akademy_registrations" id="akademy_application_avaliation_wiz" 
            sequence="33"/>
    </data>
</tryton>
