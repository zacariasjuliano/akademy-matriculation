<trytond>
	<data>
                <!-- start application_criteria_report -->
                <record model="ir.action.report" id="application_criteria_report">
                        <field name="name">Critério de admissão</field>
                        <field name="report_name">akademy_report.application.criteria.report</field>
                        <field name="model">akademy_configuration.lective.year</field>
                        <field name="report">akademy_matriculation/reports/application_criteria.odt</field>
                </record>
                <record model="ir.action.keyword" id="application_criteria_report_keyword">
                        <field name="keyword">form_print</field>
                        <field name="model">akademy_configuration.lective.year,-1</field>
                        <field name="action" ref="application_criteria_report"/>
                </record>

                <!-- start candidate_report -->
                <record model="ir.action.report" id="candidate_report">
                        <field name="name">Ficha do candidato</field>
                        <field name="report_name">akademy_report.candidates.report</field>
                        <field name="model">akademy_matriculation.candidates</field>
                        <field name="report">akademy_matriculation/reports/candidate.odt</field>
                </record>
                <record model="ir.action.keyword" id="candidate_report_keyword">
                        <field name="keyword">form_print</field>
                        <field name="model">akademy_matriculation.candidates,-1</field>
                        <field name="action" ref="candidate_report"/>
                </record>

                <!-- start applications_result_report -->
                <record model="ir.action.report" id="applications_result_report">
                        <field name="name">Pauta de exame de acesso</field>
                        <field name="report_name">akademy_report.application.result.report</field>
                        <field name="model">akademy_configuration.lective.year</field>
                        <field name="report">akademy_matriculation/reports/applications_result.odt</field>
                </record>
                <record model="ir.action.keyword" id="applications_result_report_keyword">
                        <field name="keyword">form_print</field>
                        <field name="model">akademy_configuration.lective.year,-1</field>
                        <field name="action" ref="applications_result_report"/>
                </record>

                <!-- start student_transfer_report -->
                <record model="ir.action.report" id="student_transfer_report">
                        <field name="name">Transferência externa</field>
                        <field name="report_name">akademy_report.student.transfer.report</field>
                        <field name="model">akademy_matriculation.student.transfer</field>
                        <field name="report">akademy_matriculation/reports/student_transfer.odt</field>
                </record>
                <record model="ir.action.keyword" id="student_transfer_report_keyword">
                        <field name="keyword">form_print</field>
                        <field name="model">akademy_matriculation.student.transfer,-1</field>
                        <field name="action" ref="student_transfer_report"/>
                </record>
                <record model="ir.action.report" id="student_transfer_internal_report">
                        <field name="name">Transferência interna</field>
                        <field name="report_name">akademy_report.student.transfer.internal.report</field>
                        <field name="model">akademy_matriculation.student.transfer</field>
                        <field name="report">akademy_matriculation/reports/student_transfer_internal.odt</field>
                </record>
                <record model="ir.action.keyword" id="student_transfer_internal_report_keyword">
                        <field name="keyword">form_print</field>
                        <field name="model">akademy_matriculation.student.transfer,-1</field>
                        <field name="action" ref="student_transfer_internal_report"/>
                </record>

                <!-- start equivalence_discipline_report -->
                <record model="ir.action.report" id="equivalence_discipline_report">
                        <field name="name">Gerar equivalência</field>
                        <field name="report_name">akademy_report.equivalence.discipline.report</field>
                        <field name="model">akademy_matriculation.student.transfer</field>
                        <field name="report">akademy_matriculation/reports/equivalence_discipline.odt</field>
                </record>
                <record model="ir.action.keyword" id="equivalence_discipline_report_keyword">
                        <field name="keyword">form_print</field>
                        <field name="model">akademy_matriculation.student.transfer,-1</field>
                        <field name="action" ref="equivalence_discipline_report"/>
                </record>
	</data>
</trytond>
